<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DrawableStates">
        <!-- State value for {@link android.graphics.drawable.StateListDrawable StateListDrawable},
             set when a view has input focus. -->
        <attr name="state_error" format="boolean" />
    </declare-styleable>

    <attr name="labelTextAppearance"></attr>

    <declare-styleable name="AppEditText">
        <attr name="compoundDrawableLeftTitle" format="string" />
        <attr name="compoundDrawableRightTitle" format="string" />
        <attr name="compoundDrawableRightBehaviour" format="enum">
            <enum name="none" value="0" />
            <enum name="clear" value="1" />
            <enum name="password" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SuperTextDrawable">
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:text" />
        <attr name="android:textAppearance" />
        <attr name="android:gravity" />
        <attr name="android:padding" />
        <attr name="android:paddingHorizontal" />
        <attr name="android:paddingVertical" />
        <attr name="android:font" />
        <attr name="android:fontFamily" />
        <attr name="android:textStyle" />
    </declare-styleable>

    <declare-styleable name="CustomTextAppearance">
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:text" />
        <attr name="android:gravity" />
        <attr name="android:padding" />
        <attr name="android:paddingHorizontal" />
        <attr name="android:paddingVertical" />
        <attr name="android:font" />
        <attr name="android:fontFamily" />
        <attr name="android:textStyle" />
    </declare-styleable>

</resources>